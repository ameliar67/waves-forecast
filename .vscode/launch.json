{
    "configurations": [
        {
            "name": "Attach using Process Id",
            "type": "debugpy",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "justMyCode": false
        },
        {
            "name": "Run and Debug Backend",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": false,
            "cwd": "${workspaceFolder}/backend",
            "module": "main"
        }
    ],
    "version": "0.2.0"
}